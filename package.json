{
  "name": "server",
  "version": "1.0.0",
  "description": "mern start",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "test": "echo \"Error: no test specified\" && exit 1",
    "app": "nodemon --exec node server.js",
    "start:prod": "node server.js",
    "client-install": "npm install --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alfarokaleyada/Tracker.MERN.git"
  },
  "author": "Farok Eyada",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alfarokaleyada/Tracker.MERN/issues"
  },
  "homepage": "https://github.com/alfarokaleyada/Tracker.MERN#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cache-control": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-cache-control": "^1.0.2",
    "if-env": "^1.0.4",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-bootstrap": "^1.0.1",
    "uuid": "^7.0.3",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
